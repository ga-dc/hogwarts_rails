<h1>Update Student Info</h1>

<%= form_for @student do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.submit "Update" %>
  <% end %>

  <%= form_for @student, method: :delete do |f| %>
  <%= f.submit "Delete Student", class: "delete" %>
  <% end %>
