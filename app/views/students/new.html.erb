<h1>Welcome to the Sorting Hat!</h1>
<% image_tag "http://flavorwire.files.wordpress.com/2009/06/sorting-hat.jpg" %>

<h1> Create New Student </h1>

<%= form_for @student do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label "Image URL" %>
  <%= f.text_field :img_url %>


  <%= f.submit "Create" %>

  <% end  %>
